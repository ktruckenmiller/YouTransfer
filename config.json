{
	"PORT": 5000,

	"title": "YouTransfer",
	"subtitle": "The self-hosted file sharing solution",
	"baseUrl": "http://localhost:5000",
	

	"dropzone": {
		"maxFilesize": "2000",
		"parallelUploads": 20,
		"forceFallback": false
	},

	"email": {
		"subject": "YouTransfer",
		"transporter": "direct",
		"sendmailPath": "/usr/sbin/sendmail",
		"region": "eu-west-1",
		"rateLimit": 1
	},

	"retention": 1,
	"retentionUnit": "weeks",
	"cleanupSchedule": "*/1 * * * *",
	"enableDownload": "true",
	"StorageLocation": "local",
	"localstoragepath": "./uploads",
	"S3Region": "us-east-1",
	"S3SSLEnabled": "true"
}